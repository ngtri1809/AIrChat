# AIrChat Python Service Environment Variables
# Copy this file to .env and replace the placeholder values with your actual API keys

# OpenAQ API Configuration
OPENAQ_API_KEY=your_openaq_key_here

# LLM API Keys (replace with your actual keys)
OPENAI_API_KEY=your_openai_key_here
GOOGLE_API_KEY=your_google_key_here

# LLM Model Configuration
OPENAI_MODEL=gpt-4o-mini
GOOGLE_MODEL=gemini-1.5-flash

# Default LLM Provider
LLM_PROVIDER=google

# Service Configuration
HOST=0.0.0.0
PORT=8000

